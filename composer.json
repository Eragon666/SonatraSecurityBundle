    "keywords": ["ACL", "ACE", "Advanced ACL Concept Implementation", "Rule definition", "Sharing rule", "ACL Voter", "JMS Expression"],
    "homepage": "http://sonatra.com",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3",
        "friendsofsymfony/user-bundle": "~2.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "~1.3"
